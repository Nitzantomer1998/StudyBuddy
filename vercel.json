{
  "builds": [
    {
      "src": "studybuddy/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.11.4" }
    },
    {
      "src": "vercel_build.sh",
      "use": "@vercel/static",
      "config": { "distDir": "staticfiles" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "studybuddy/wsgi.py"
    }
  ]
}
